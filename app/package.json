{
  "name": "hal",
  "description": "HAL Management Console",
  "private": true,
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "parcelIgnore": [
    "hal.nocache.js"
  ],
  "source": [
    "src/web/dev.html",
    "src/web/index.html",
    "src/web/ts.html"
  ],
  "scripts": {
    "watch": "parcel watch --public-url \"./\" --dist-dir target/gwt/devmode/war/hal",
    "dev": "parcel build --public-url \"./\" --dist-dir target/gwt/devmode/war/hal --no-optimize",
    "prod-eap": "cross-env THEME=eap parcel build --public-url \"./\" --dist-dir target/parcel",
    "prod-hal": "cross-env THEME=hal parcel build --public-url \"./\" --dist-dir target/parcel",
    "prod-wildfly": "cross-env THEME=wildfly parcel build --public-url \"./\" --dist-dir target/parcel"
  },
  "devDependencies": {
    "@parcel/reporter-cli": "^2.3.1",
    "@parcel/transformer-less": "^2.3.1",
    "cross-env": "^7.0.3",
    "parcel": "^2.3.1",
    "parcel-resolver-ignore": "^2.0.0",
    "parcel-resolver-theme": "file:local_modules/parcel-resolver-theme"
  },
  "dependencies": {
    "ace-builds": "^1.4.14",
    "bootstrap": "~3.4.1",
    "bootstrap-select": "^1.13.18",
    "bootstrap-switch": "^3.4.0",
    "c3": "^0.7.20",
    "d3": "5.9.7",
    "datatables.net": "^1.10.19",
    "datatables.net-buttons": "^1.5.6",
    "datatables.net-keytable": "^2.5.0",
    "datatables.net-select": "^1.3.0",
    "events": "^3.3.0",
    "font-awesome": "^4.7.0",
    "google-code-prettify": "^1.0.5",
    "jquery": "~3.4.1",
    "js-cookie": "^3.0.1",
    "jstree": "^3.3.12",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "node-fetch": "2.6.7",
    "patternfly": "^3.59.5",
    "pouchdb-browser": "^7.2.2",
    "zeroclipboard": "^2.3.0"
  }
}
